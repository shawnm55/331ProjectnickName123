{
	"manifest_version": 2,
	"version": "1.0",
	"name": "331Project",
	"icons": {
		"128": "ate_someting_bad.png"
	},
	"content_scripts": [{
		"matches": ["<all_urls>"],
		"js": ["pinkattack.js"]
	}],
	"background": {
		"scripts": ["background.js"]
	},
	
	"chrome_url_overrides": {
        "newtab": "newtab.html"
    },

    "permissions": [
        "activeTab",
        "history",
        "storage"
    ]

}